package com.trifulcas.id;

public class MainColores {

	public static void main(String[] args) {
		
		 Jugador eva = new Jugador("Eva", new Teclado());
         Jugador juan = new Jugador("Juan", new Teclado());
         Juego colores = new Juego(eva, juan, new Colores());
         System.out.println(colores.jugar());

	}

}
